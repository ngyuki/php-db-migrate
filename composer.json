{
    "name": "ngyuki/db-migrate",
    "description": "Simple migration tool",
    "type": "library",
    "keywords": [ "database", "migrations" ],
    "homepage": "https://github.com/ngyuki/php-db-migrate",
    "license": "MIT",
    "authors": [
        { "name": "ngyuki", "email": "ngyuki.ts@gmail.com" }
    ],
    "config": {
        "platform": {
            "php": "5.3.3"
        }
    },
    "require": {
        "php": ">=5.3.3",
        "symfony/console": "~2.6",
        "symfony/filesystem": "~2.6",
        "doctrine/dbal": "~2.5"
    },
    "require-dev": {
        "symfony/finder": "~2.6"
    },
    "autoload": {
        "psr-4": {
            "ngyuki\\DbMigrate\\": "src/"
        }
    },
    "bin": [
        "bin/db-migrate"
    ]
}
